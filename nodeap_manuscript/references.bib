@article{RN547,
   author = {Rubin, R. D. and Watson, P. D. and Duff, M. C. and Cohen, N. J.},
   title = {The role of the hippocampus in flexible cognition and social behavior},
   journal = {Front Hum Neurosci},
   volume = {8},
   pages = {742},
   note = {Rubin, Rachael D
Watson, Patrick D
