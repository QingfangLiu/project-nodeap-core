@article{RN267,
   author = {Bridwell, David A. and Srinivasan, Ramesh},
   title = {Distinct Attention Networks for Feature Enhancement and Suppression in Vision},
   journal = {Psychological Science},
   volume = {23},
   number = {10},
   pages = {1151-1158},
   abstract = {Attention biases sensory processing toward neurons containing information about behaviorally relevant events. These attentional biases apparently reflect the combined influence of feature enhancement and suppression. We examined the separate influence of enhancement and suppression in visual processing by determining whether responses to an unattended flicker were modulated when the flicker features matched target features at the attended location, competed with those features, or were neutral. We found that suppression primarily modulated parietal networks with a preferred frequency in the lower alpha band (f2 = 8 Hz), and enhancement primarily influenced parietal networks with a preferred frequency in the upper alpha band (f2 = 12 Hz). These responses were coupled with perception, with large responses to the unattended flicker leading to subsequently detected targets when the target features matched the flicker features (i.e., during enhancement). Our results suggest that enhancement and suppression are two distinct processes that work together to shape visual perception.},
   ISSN = {0956-7976, 1467-9280},
   DOI = {10.1177/0956797612440099},
   url = {http://journals.sagepub.com/doi/10.1177/0956797612440099},
   year = {2012},
   type = {Journal Article}
}

